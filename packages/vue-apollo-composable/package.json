    "@vue/composition-api": "^1.0.0-beta.16",
    "graphql": "^14.5.8",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@apollo/client": "^3.0.0",
    "@types/throttle-debounce": "^2.1.0",
    "@vue/composition-api": "^1.0.0-beta.16"
  }
